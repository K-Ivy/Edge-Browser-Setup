{
	"manifest_version": 3,
	"name": "Newtab Redirect",
	"version": "1.0",
	"description": "Redirects the Edge Newtab page to my Everforest Startpage.",
	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},
	"icons": {
		"16": "icons/icon_16.png",
		"32": "icons/icon_32.png",
		"48": "icons/icon_48.png",
		"128": "icons/icon_128.png"
	}
}